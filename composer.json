{
    "name": "mayifit/core-translation",
    "description": "Core translation system for Laravel based MayIFit systems",
    "keywords": ["translation", "spatie", "laravel", "php", "management"],
    "type": "project",
    "authors": [
        {
            "name": "MayIFit",
            "email": "contact@mayifit.net"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/MayIFit/core-permission"
        }
    ],
    "autoload": {
        "psr-4": {
            "MayIFit\\Core\\Translation\\": "src/"
        }
    },
    "require": {
        "php": "^7.2",
        "spatie/laravel-translation-loader": "^2.4",
        "laravel/helpers": "^1.2",
        "mayifit/core-permission": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "MayIFit\\Core\\Translation\\TranslationServiceProvider"
            ],
            "aliases": {
                "Translation": "MayIFit\\Core\\Translation\\Facades\\Translation"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
