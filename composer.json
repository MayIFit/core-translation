{
    "name": "mayifit/core-translation",
    "description": "Core translation system for Laravel based MayIFit systems",
    "keywords": ["permission", "laravel", "php", "roles", "management"],
    "type": "project",
    "authors": [
        {
            "name": "MayIFit",
            "email": "contact@mayifit.net"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/MayIFit/core-permission"
        }
    ],
    "autoload": {
        "psr-4": {
            "MayIFit\\Core\\Translation\\": "src/",
            "MayIFit\\Core\\Translation\\Database\\Seeds\\": "src/Database/Seeds/",
            "MayIFit\\Core\\Translation\\Facades\\": "src/Facades/",
            "MayIFit\\Core\\Translation\\Models\\": "src/Models/",
            "MayIFit\\Core\\Translation\\Policies\\": "src/Policies/"
        }
    },
    "require": {
        "php": "^7.0",
        "spatie/laravel-translation-loader": "^2.4",
        "laravel/helpers": "^1.2",
        "mayifit/core-permission": "^1.0"
    },
    "require-dev": {
        "laravel/framework": "^6.0 || ^7.0",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^5.3",
        "orchestra/database": "^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MayIFit\\Core\\Translation\\TranslationServiceProvider"
            ],
            "aliases": {
                "Translation": "MayIFit\\Core\\Translation\\Facades\\Translation"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
